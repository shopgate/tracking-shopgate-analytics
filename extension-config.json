{
  "version": "1.3.0-beta.1",
  "id": "@shopgate/tracking-shopgate-analytics",
  "components": [
    {
      "id": "init",
      "path": "frontend/src/index.js",
      "type": "tracking"
    }
  ],
  "configuration": {
    "stage": {
      "type": "static",
      "destination": "frontend",
      "params": {
        "value": "%(targetStage)s"
      }
    },
    "overrideStage": {
      "type": "admin",
      "destination": "frontend",
      "default": null,
      "params": {
        "type": "text",
        "label": "The target stage for all tracking requests"
      }
    },
    "overrideShopNumber": {
      "type": "admin",
      "destination": "frontend",
      "default": null,
      "params": {
        "type": "text",
        "label": "An alternative target shop number for all tracking requests"
      }
    },
    "debugEnabled": {
      "type": "admin",
      "destination": "frontend",
      "default": false,
      "params": {
        "type": "checkbox",
        "label": "Enables debug mode"
      }
    }
  }
}
